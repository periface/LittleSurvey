@model IEnumerable<Survey.Application.SurveyService.Dto.SurveyDto>

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout_Survey.cshtml";
}

@foreach (var sr in Model)
{
    <li class="list-group-item">
        <a href="@Url.Action("LoadSurvey", new {sr.SurveyUrl})">@sr.Description</a>
                    
    </li>
}